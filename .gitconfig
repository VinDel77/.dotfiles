[alias]

  a = add
  b = branch
  c = commit
  co = checkout
  p = push
  st = status

  ap = add --patch
  ulc = reset --soft HEAD~1

  lgp = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit

  la = "!git config -l | grep alias | cut -c 7-"
